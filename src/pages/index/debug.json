        }
      },
      {
        "name": "d",
        "comment": "直径",
        "source": {
          "type": "mathjs",
          "expression": "(l_heng_a+l_zong_a)/2"
        }
      },
      {
        "name": "F",
        "comment": "扭转模量",
        "source": {
          "type": "mathjs",
          "expression": "pi * d ^ 4 * 7.9 * 10^10 / (32 * l) / 1000000"
        }
      },
      {
        "name": "J_0",
        "comment": "圆台转动惯量",
        "source": {
          "type": "mathjs",
          "expression": "F * T_0_t^2 / (4 * pi^2)"
        }
      },
      {
        "name": "J",
        "comment": "摆+环转动惯量",
        "source": {
          "type": "mathjs",
          "expression": "F*T_t^2/ (4 * pi^2)"
        }
      },
      {
        "name": "J_1",
        "comment": "圆环转动惯量",
        "source": {
          "type": "mathjs",
          "expression": "J-J_0"
        }
      },
      {
        "name": "J_1_lilun",
        "comment": "𝐽₁理论值",
        "source": {
          "type": "mathjs",
          "expression": "0.5 * m * (R_1^2 + R_2^2) / 1000000"
        }
      },
      {
        "name": "E",
        "comment": "相对误差",
        "source": {
          "type": "mathjs",
          "expression": "abs((J_1_lilun-J_1)/J_1)*100"
        }
      }
    ]
